    {"name": "ActiveCard",    "supertypes": [ "CardFront" ], "abstract": true},
    {"name": "AutomatedCard", "supertypes": [ "CardFront" ], "abstract": true},
    {"name": "EventCard",     "supertypes": [ "CardFront" ], "abstract": true},

