    {
      "name": "SoloPlayer",
      "supertypes": [ "Player" ]
    },
    {
      "name": "Owned",
      "dependencies": [ "Anyone" ], // 1: owned to owner
      "abstract": true
    },

    {
      "name": "Phase",
      "system": true,
      "abstract": true
    },
    {
      "name": "ProductionPhase",
      "supertypes": [ "Phase" ]
    },
    {
      "name": "End",
      "supertypes": [ "Phase" ]
    },

    {
      "name": "Generation",
      "supertypes": [ "Plural" ]
    },
    {
      "name": "VictoryPoint",
      "supertypes": [ "Owned<Player>", "Plural" ]
    },
    {
      "name": "TerraformRating",
      "supertypes": [ "Owned<Player>", "Plural" ],
      "effects": [
        "ProductionPhase: 1",
        "End: VictoryPoint"
      ]
    },

    {
      "name": "Resource",
      "supertypes": [ "Owned", "Plural" ],
      "abstract": true
    },
    {
      "name": "StandardResource",
      "supertypes": [ "Resource" ],
      "abstract": true
    },
    {"name": "Megacredit", "supertypes": ["StandardResource"]},
    {"name": "Steel",      "supertypes": ["StandardResource"]},
    {"name": "Titanium",   "supertypes": ["StandardResource"]},
    {"name": "Plant",      "supertypes": ["StandardResource"]},
    {"name": "Energy",     "supertypes": ["StandardResource"]}, // TODO: the whole becoming Heat thing
    {"name": "Heat",       "supertypes": ["StandardResource"]},
    {
      "name": "Production",
      "supertypes": [ "Owned<Anyone>, Plural" ],
      "dependencies": [ "Ctype<StandardResource>" ], // 2: production to resource type
      "effects": [
        "ProductionPhase: StandardResource"
      ]
