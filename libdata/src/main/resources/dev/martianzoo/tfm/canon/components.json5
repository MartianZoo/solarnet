    {
      "name": "TemperatureStep",
      "supertypes": [ "GlobalParameter" ],
      "effects": [
        // "This IF =3 This: PROD[Heat]",
        // "This IF =5 This: PROD[Heat]",
        // "This IF =15 This: OceanTile",
      ]
